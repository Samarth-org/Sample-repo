name: first workflow

on:
  push

jobs:
  build-job-1:
    runs-on: ubuntu-latest
    steps:
    - name: Install npm package cowsay program
      run: sudo apt-get install cowsay -y

    - name: excecute cowsay CMD
      run: cowsay -f dragon "run for cover ......I am dragon" >> dragon.txt 
      
